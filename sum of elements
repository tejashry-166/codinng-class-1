import java.util.*;
public class sumelements
{
   public static void main(String args[])
   {
    int arr[]={10,15,20,25,30,35,40};
    int sum=0;
    System.out.println("Sum of array elements are : ");
    for(int i=0;i<arr.length;i++)
    {
        sum=sum+arr[i];
    }
    System.out.print(sum);
   }
}
